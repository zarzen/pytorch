   {
      "name": "iOS",
      "patterns": ["ios/**", "test/mobile/**"],
      "approved_by": ["linbinyu", "kit1980", "xta0", "hanton"],
      "mandatory_checks_name": ["Facebook CLA Check"]
   },
   {
      "name": "superuser",
      "patterns": ["*"],
      "approved_by": ["albanD", "jbschlosser", "suo", "osalpekar", "malfet", "seemethere", "ezyang"],
      "mandatory_checks_name": ["Facebook CLA Check"]
